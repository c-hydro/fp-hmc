    "application_variable_env": {
      "run_domain": null,
      "run_time_now": null,
      "run_time_start": null,
      "run_time_end": null,
      "run_time_step_obs": null,
      "run_time_step_for": null,
      "run_mount_folder_static": null,
      "run_mount_folder_dynamic_source": null,
      "run_mount_folder_dynamic_destination": null,
      "run_mount_folder_dynamic_restart": null
    },
    "application_variable_local": {
      "run_domain": "med_d01",
      "run_time_now": "2021-12-31 23:00",
      "run_time_start": "2015-01-01 01:00",
      "run_time_end": "2021-12-31 23:00",
      "run_time_step_obs": null,
      "run_time_step_for": null,
